{
	"name": "1of0/json",
	"description": "Abstraction layer for json_encode() and json_decode(), providing Json.NET-like functionality through annotations or mappings",
	"keywords": [ "json", "serializer", "serialization", "json.net", "annotations" ],
	"homepage": "https://bitbucket.org/1of0/json",
	"license": "MIT",
	"authors": [
		{
			"name": "Bernardo van der Wal",
			"email": "composer@thebigb.nl"
		}
	],
	"require":
	{
		"php": ">=5.5.0",
		"ext-json": "*",
		"1of0/better-annotations": "^0.1.0",
		"1of0/phpdoc-reader": "^3.0.0",
		"doctrine/annotations": "^1.2.0",
		"container-interop/container-interop": "^1.0.0"
	},
	"require-dev":
	{
		"apigen/apigen": "^4.1.2",
		"nesbot/carbon": "^1.0.0",
		"phpunit/phpunit": "^4.8.0",
		"php-di/php-di": "^5.1.0",
		"doctrine/cache": "^1.3.0",
		"ocramius/proxy-manager": "^1.0.0",
		"symfony/yaml": "^2.7.0 || ^3.0.0",
		"danielstjules/stringy": "^2.0.0"
	},
	"suggest":
	{
		"ext-xml": "Required for XML mapper support",
		"symfony/yaml": "Required for YAML mapper support",
		"danielstjules/stringy": "Required for PascalCaseContractResolver",
		"doctrine/cache": "Required for mapper caching",
		"ocramius/proxy-manager": "Useful for making lazy proxies"
	},
	"autoload":
	{
		"psr-4": { "OneOfZero\\Json\\": "src/" }
	},
	"autoload-dev":
	{
		"psr-4": { "OneOfZero\\Json\\Test\\": "test/" }
	}
}
